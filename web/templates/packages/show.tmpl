                    <!-- end -->

                    <!-- render partial: 'maintainer_needed_notice', object: @package, as: 'package' -->

                    {{template "metadata" .}}

                    <!-- render partial: 'masks', object: @package.versions, as: 'versions' -->

                    {{template "changelogwrapper" .}}

