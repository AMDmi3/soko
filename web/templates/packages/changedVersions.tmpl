<!DOCTYPE html>
<html lang="en">
{{template "head" .Header}}
<body>
{{template "header" .Header}}



<div class="container mb-5">
    <div class="row">
        <div class="col-12">

            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="/">Home</a></li>
                <li class="breadcrumb-item"><a href="/categories">Packages</a></li>
                <li class="breadcrumb-item active">{{.Name}} Packages</li>
            </ol>

            <h1>
                {{.Name}} Packages
                <a title="Atom feed" href="/packages/{{tolower .Name}}.atom" class="kk-feed-icon"><span class="fa fa-fw fa-rss-square"></span></a>
            </h1>

            <li class="list-group">
                {{range .Versions}}
                    {{template "changedversion" .}}
                {{end}}

            </li>


        </div>
    </div>
</div>


{{template "footer" .Application }}

</body>
</html>
